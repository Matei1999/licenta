{
  "name": "sleep-apnea-management",
  "version": "1.0.0",
  "description": "Sleep Apnea Management System - Patient monitoring and data tracking",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "install:all": "npm install && cd frontend && npm install",
    "seed:bulk": "node backend/scripts/seedBulkPatients.js",
    "cleanup:osa": "node backend/scripts/cleanupMildOrNormalPatients.js"
  },
  "keywords": [
    "sleep-apnea",
    "healthcare",
    "patient-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
